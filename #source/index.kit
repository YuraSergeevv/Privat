<!DOCTYPE html>
<!-- Оболочка документа -->
<html lang="ru">

<!-- Заголовок страницы, контейнер для других важных данных (не отображается) -->

<head>
    <title>Главная</title>
    <!-- Подключение CSS -->
    <link rel="stylesheet" href="css/style.css">
    <meta http-equiv="Content-type" content="text/html;charset=UTF-8"/>
    <link rel="shortcut icon" href="img/icons/favicon.ico">
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <meta name="mobile-web-app-capable" content="yes">
</head>
<body>

<!-- ОБОЛОЧКА -->
<div class="wrapper">
    <!-- @import "header.kit" -->
    <div class="container">
        <div class="content_1 ">
            <div class="card_inner">
                <div class="card">
                    <div class="card_top">
                        <div class="img_block">
                            <img src="./img/1.png" alt="">
                        </div>
                        <div class="block_text">
                            <div class="txt">Херсон</div>
                            <div class="txt">Херсонелектротранс</div>
                            <div class="txt "><span class="grey">Серия</span> <span class="series series-out_js"></span>
                            </div>
                        </div>
                    </div>
                    <div class="card_center">
                        <div class="img_block">
                            <img src="./img/2.jpg" alt="">
                        </div>
                        <div class="number number-out_js"></div>
                        <div class="type">Троллейбус</div>
                    </div>
                    <div class="card_bottom">
                        <div class="info_block">
                            <div class="title">Дата</div>
                            <div class="item date-out_js"></div>
                        </div>
                        <div class="info_block">
                            <div class="title">Время</div>
                            <div class="item time-out_js"></div>
                        </div>
                        <div class="info_block">
                            <div class="title">Стандартный</div>
                            <div class="item count-out_js"></div>
                        </div>
                    </div>
                    <div class="warning">Билет разового использования</div>
                    <div class="timer timer-out_js"></div>
                </div>
                <div class="card last">
                    <div class="card_top">
                        <div class="img_block">
                            <img src="./img/1.png" alt="">
                        </div>
                        <div class="block_text">
                            <div class="txt">Херсон</div>
                            <div class="txt">Херсонелектротранс</div>
                            <div class="txt "><span class="grey">Серия</span> <span class="series series-out_js"></span>
                            </div>
                        </div>
                    </div>
                    <div class="card_center">
                        <div class="img_block">
                            <img src="./img/2.jpg" alt="">
                        </div>
                        <div class="number number-out_js"></div>
                        <div class="type">Троллейбус</div>
                    </div>
                    <div class="card_bottom">
                        <div class="info_block">
                            <div class="title">Дата</div>
                            <div class="item date-out_js"></div>
                        </div>
                        <div class="info_block">
                            <div class="title">Время</div>
                            <div class="item time-out_js"></div>
                        </div>
                        <div class="info_block">
                            <div class="title">Стандартный</div>
                            <div class="item count-out_js"></div>
                        </div>
                    </div>
                    <div class="warning">Билет разового использования</div>
                    <div class="timer timer-out_js"></div>
                </div>
            </div>
        </div>

        <div class="content_2 active">
            <div class="input_block">
                <div class="block_input">
                    <input class="input number-in_js" type="number" placeholder="Ведите даные">
                    <div class="text">Номер</div>
                </div>
                <div class="block_input">
                     <input class="input count-in_js" type="number" placeholder="Ведите даные">
                     <div class="text">Количество</div>
                 </div>
                <div class="btn_ok">OK</div>
            </div>
        </div>
        <div class="content_3 ">
            <div class="inner">
                <div class="block ">
                    <div class="block_inner">
                        <div class="top">
                            <div class="text">Проезд</div>
                            <div class="price"><span class="num"><span class="big">5.</span>00</span><span class="val">грн</span>
                            </div>
                        </div>
                        <div class="bottom">
                            <div class="title">Количество билетов</div>
                            <div class="input_block">
                                <div class="input_wrap">
                                    <input class="input count-in_js" type="number" value="1">
                                    <span class="сount">шт</span>
                                </div>
                                <div class="btn_block">
                                    <div class="btn plus"></div>
                                    <div class="btn minus"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="info">
                    <div class="info_top">
                        <div class="text">К оплате:</div>
                        <div class="final_price">
                            <div class="price"><span class="final_num"><span class="big">7.</span>00</span><span class="final_val">грн</span>
                            </div>
                        </div>
                    </div>
                    <div class="card_line">
                        <div class="card_number">*5347 Карта для выплат</div>
                        <div class="car_money">
                            <span class="money">33грн</span>
                            <span class="card_arrow"></span>
                        </div>
                    </div>
                    <div class="btn_buy">Купить</div>
                </div>
            </div>
        </div>
    </div>
    <!-- @import "popup.kit" -->

    <!-- @import "bottom.kit" -->

</body>

</html>